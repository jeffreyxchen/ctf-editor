{
  "name": "ctf-editor",
  "version": "1.0.0",
  "description": "A minimal Electron application with React and Express",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently -k \"npm run devWebpack\" \"npm run devApp\" \"npm run devServer\"",
    "devWebpack": "webpack-dev-server --content-base build/",
    "devApp": "electron main.js --dev",
    "devServer": "nodemon --watch backend backend/server.js",
    "webpack": "webpack",
    "prodWebpack": "webpack -p",
    "start": "electron main.js",
    "server": "node backend/server.js"
  },
  "repository": "https://github.com/brianfakhoury/ctf-editor.git",
  "keywords": [],
  "author": "Jeffrey Chen, Brian Fakhoury, Jay Tang",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.0",
    "draft-js": "^0.10.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "loglevel": "^1.4.1",
    "nodemon": "^1.11.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.6.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "draft-js": "^0.10.1",
    "electron": "~1.6.2",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "mongoose": "^4.11.4",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
