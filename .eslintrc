{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-console": 0,
        "react/jsx-filename-extension": 0
    },
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    }
}
